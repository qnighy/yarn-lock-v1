{
  "name": "yarn-lock-v1",
  "version": "0.1.0",
  "description": "A tool to downgrade your yarn.lock",
  "main": "dist/index.js",
  "repository": "https://github.com/qnighy/yarn-lock-v1",
  "author": "Masaki Hara <ackie.h.gmai@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && babel src -d dist -x .ts --ignore \"src/**/*.test.ts\"",
    "fmt": "$npm_execpath run fmt:runprettier -w",
    "fmt:check": "$npm_execpath run fmt:runprettier -c",
    "fmt:runprettier": "prettier \"src/**/*.ts\" .babelrc.js .eslintrc.js tsconfig.json tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\" .babelrc.js .eslintrc.js --ignore-pattern \"!.babelrc.js\"",
    "lint:fix": "$npm_execpath run lint --fix",
    "prepack": "$npm_execpath run build",
    "start": "$npm_node_execpath dist/index.js",
    "start:dev": "babel-node -x .ts -- src/index.ts",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.17",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@jest/globals": "^26.6.2",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@yarnpkg/pnpify": "^3.0.0-rc.3",
    "babel-jest": "^26.6.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  }
}
