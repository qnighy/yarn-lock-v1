{
  "name": "yarn-lock-v1",
  "version": "0.1.0",
  "description": "A tool to downgrade your yarn.lock",
  "main": "dist/index.js",
  "repository": "https://github.com/qnighy/yarn-lock-v1",
  "author": "Masaki Hara <ackie.h.gmai@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && babel src -d dist -x .ts",
    "prepack": "$npm_execpath run build",
    "start": "$npm_node_execpath dist/index.js",
    "start:dev": "babel-node -x .ts -- src/index.ts"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.17",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@types/yargs": "^16.0.1",
    "@yarnpkg/pnpify": "^3.0.0-rc.3",
    "typescript": "^4.2.4"
  }
}
